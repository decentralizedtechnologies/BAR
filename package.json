{
  "name": "bar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "next dev -p 3000 & npm run scss:class-types:watch",
    "build": "next build",
    "start": "next start",
    "scss:class-types": "tsm 'src/**/**/*.scss' --ignore 'src/theme/*.scss' --exportType default --nameFormat none --implementation node-sass src --logLevel silent",
    "scss:class-types:watch": "npm run scss:class-types -- --watch"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "dropzone": "^5.8.1",
    "ipfs-http-client": "^49.0.4",
    "next": "10.0.9",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-grid-system": "^7.1.2",
    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@types/dropzone": "^5.7.3",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "node-sass": "^5.0.0",
    "typed-scss-modules": "^4.1.1",
    "typescript": "^4.2.3"
  }
}
